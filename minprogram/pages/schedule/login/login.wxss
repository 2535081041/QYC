.container{
  height: 70vh; 
  width: 100%; 

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.form{
  width: 80%;
  background-color:#CBE0F4;

  align-items: center;
  justify-content: center;
  border-radius: 20rpx;
}
.title{
  font-size: larger;
  font-weight: bold;
  text-align: center;
  margin: 30rpx;
}
.input-box{
  background-color:#eaf5ff;
  display: flex;
  align-items: center;
  border-radius: 10rpx;
  margin: 10rpx;
  height: 70rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.img{
  height: 55rpx;
  width: 60rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.submit{
  margin: 30rpx;
  background-color: #afd6fa;
}

